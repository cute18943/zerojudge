#include<stdio.h>
#include<stdlib.h>
#include <string.h>
int main()
{
	int a,b;
	char c;
	while((scanf("%d %c %d",&a,&c,&b))!=EOF)
	{
		if(c=='+'){
			c=a+b;
			printf("%d\n",c);
		}else if(c=='-'){
			c=a-b;
			printf("%d\n",c);
		}else if(c=='*'){
			c=a*b;
			printf("%d\n",c);
		}else if(c=='/'){
			c=a/b;
			printf("%d\n",c);
		}
			
			
			
			
		
	}
return 0;
}

