#include<stdio.h>
#include<stdlib.h>

int main()
{
	int a;

	while((scanf("%d",&a))!=EOF)
	{
	if(a%5==0){

	printf("U\n");
		
	}else if(a%5==1){
	
	printf("G\n");
			
}else if(a%5==2){

printf("Y\n");
		
}else if(a%5==3){

	printf("T\n");
	
}else if(a%5==4){

printf("I\n");

	}		
	}
return 0;
}

