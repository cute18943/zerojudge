#include<stdio.h>
#include<stdlib.h>

int main(){
	
	int m,d,s;
    
while( scanf("%d%d",&m,&d)!=EOF ) {
	int s=(m*2+d)%3;
	if(s=(m*2+d)%3==0){
	 printf("普通\n");
}else if(s=(m*2+d)%3==1){
printf("吉\n");
}else if(s=(m*2+d)%3==2)
printf("大吉\n");
 }
	return 0;
}

